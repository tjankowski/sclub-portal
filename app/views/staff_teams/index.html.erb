<div class="box">
  <div class="box_header">
    <h1>
      Drużyny
    </h1>
  </div>
  <div class="box_content">
    <h3>Dodane</h3>
    <ul>
      <%= render :partial => 'team', :collection => @staff.teams %>
    </ul>
    <h3>Dostępne</h3>
    <ul>
      <%= render :partial => 'team', :collection => (@all_teams - @staff.teams) %>
    </ul>
  </div>
</div>