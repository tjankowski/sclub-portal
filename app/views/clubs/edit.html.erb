<div class="box">
  <div class="box_header">
    <h1>Klub</h1>
  </div>
  <div class="box_content">
    <% form_for :club do |f| %>
      <div class="form_row">
        <label for="name">Nazwa</label>
        <%= f.text_field :name %>
      </div>
      <div class="form_row">
        <label for="city">Miasto</label>
        <%= f.text_field :city %>
      </div>
      <div class="form_row">
        <div>
          <%= f.label :logo, "Plik:" %>
          <%= f.file_field :logo %>
        </div>
      </div>
      <div>
        <%= submit_tag "Zapisz zmiany", :class => "submit" %>
      </div>
    <% end %>
  </div>
</div>